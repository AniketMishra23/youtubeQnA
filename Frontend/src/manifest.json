{
    "manifest_version" : 3,
    "name" : "Youtube Video QnA",
    "description" : "This extension will help you to ask questions on youtube videos just like a QnA Bot",
    "version" : "1.0",
    "permissions" : [
        "activeTab"
    ],
    "icon":{
        "16": "../public/youtube.svg",
        "48": "../public/youtube.svg",
        "128": "../public/youtube.svg"
    },
    "action" : {
        "default_popup" : "index.html"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
      }
}